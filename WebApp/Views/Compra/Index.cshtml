@model WebApp.Models.ViewModels.CompraVM
@{
    ViewData["Title"] = "Registrar Compra";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Seccion de compras</h1>
<hr>

<ul>

    @foreach (var v in Model.CompraLista)
    {
        <li>Usuario: @v.Usuario.Nombre</li>
        <li>Item: @v.CompraId (@v.Producto.Nombre)</li>
        <li>Fecha: @v.Fecha</li>
        <li>Cantidad: @v.Cantidad</li>
        
    }

</ul>